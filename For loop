#include<stdio.h>
int main(){
   int i, start,sum=0,stop;
   printf("enter when to start");
   scanf("%d",&start);
   
   printf("enter when to stop");
   scanf("%d",&stop);
   
   for( i=start;i<=stop;i++){
   printf("%d\n",i);
   sum+=i;
   }
   printf("sum=%d",sum);
   
   return 0;
   }
