#include<stdio.h>
int main(){
   int i, start,stop,sum;
   printf("enter when to start");
   scanf("%d",&start);
   
   printf("enter when to stop");
   scanf("%d", & stop);
   
   i=start;
   while ( i<=stop){
   printf("%d\n",i);
   i++;
   sum+=i;
   }
   printf("sum=%d", sum);
   
   return 0;
   }
